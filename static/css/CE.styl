/* MODULO SCREEN 160PX */

@media screen and (min-width:160px)
	html
		background black
		body
			background white
		nav.Canvas
			left 0
			// overflow hidden
			position fixed
			right 0 
			top 0
			z-index 6
		.Header
			background black
			color white
			height 30px
			line-height 30px
			padding 10px 0
			position relative	
			&-logoCon
				margin 0
				top 0
				width 200px
			&-logoTexto
				display block
				font-size 30px
				left 60px
				line-height 40px
				margin 0
				position absolute
				text-shadow 0 0 10px lightgray
				top 6px
			&-logo1
				display block
				left 5px
				margin 0
				position absolute
				top 2px
				z-index 10px
				
			&-button
				background black
				border none	
				color white
				display none
				font-size 30px
				font-weight normal
				padding 0
				position absolute
				right 5px
				top 10px
				&.is-active
					display block 
		.MainMenu
			display visible
			background black
			height auto
			left -200px
			list-style none
			margin 0
			padding 0
			position absolute
			position fixed
			top 50px
			transition .2s
			width 160px
			&.is-active
				left 0
			&-item
				-webkit-transition .2s
				height 50px
				line-height 50px
				text-align center
				text-shadow 0 0 10px Verde_D
				width 150px
				&:hover
					background lightgray	
			&-link
				color white
				display block
				font-size 14px
				text-decoration none
				text-transform uppercase

/* ESTILOS DE CE.HTML */
		h1.CE-title
			background black
			color white
			text-align center
			font-size 100%
			font-family Oswald
