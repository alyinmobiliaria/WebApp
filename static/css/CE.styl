/* STYLUS DEL INDEX.HTML, CLASIFICADOS.HTML, TRABAJOS.HTML, NOTICIAS.HTML,
TANTO DEL NAV, COMO DEL HEADER, EL MENU Y EL AVATAR */
body
	background lightgray
	nav.Canvas
		left 0
		// overflow hidden
		position fixed
		right 0 
		top 0
		z-index 6
	.Header
		background Green_Aly
		color white
		height 30px
		line-height 30px
		padding 10px 0
		position relative	
		&-logoCon
			margin 0
			top 0
			width 200px
		&-logoTexto
			display block
			font-size 30px
			left 60px
			line-height 40px
			margin 0
			position absolute
			text-shadow 0 0 10px Verde_D
			top 6px
		&-logo1
			display block
			left 5px
			margin 0
			position absolute
			top 2px
			z-index 10px
		&-button
			background Green_Aly
			border none	
			color white
			display none
			font-size 30px
			font-weight normal
			padding 0
			position absolute
			right 5px
			top 10px
			&.is-active
				display block 
		.MainMenu
			display visible
			background Green_Aly
			height auto
			left -200px
			list-style none
			margin 0
			padding 0
			position absolute
			position fixed
			top 50px
			transition .2s
			width 200px
			&.is-active
				left 0
			&-item
				-webkit-transition .2s
				height 50px
				line-height 50px
				text-align center
				text-shadow 0 0 10px Verde_D
				&:hover
					background lightgray	
			&-link
				color white
				display block
				font-size 14px
				text-decoration none
				text-transform uppercase	
	#Avatar
		background-image url("../images/portada.png")	
		background-position center
		background-repeat no-repeat
		background-size cover
		height auto
		margin-top 47px
		overflow hidden 
		padding 1.5em 0
		text-align: center
		.Avatar-titulo
			background rgba(255, 255, 255, 0.38)
			border 5px solid black
			display inline-block
			height auto
			margin-left 0
			margin-right 0
			margin-top 0
			text-align center
			width 300px
		.Avatar-img
			background rgba(255, 255, 255, 0.38)
			border 5px solid black
			border-radius 10%
/* CARDÓN ESTÉREO */
	h2.Service-title
		margin-top 0px
		margin-bottom 0px
		background black
		color white
		text-align center
	section.Service-CE
		background white
		text-align center
	article.Service-Container
		div.Service-Descri
			padding 10px
			background rgba(0, 0, 0, 0.73)
			width 160px
			height 230px
			margin-left auto
			margin-right auto
			margin-top 10px
			margin-bottom 10px
		h2.Service-title
			color white
		h3.Service-title
			color white
		p
			color white
		a.Service-link
			text-decoration none
			color white
			width 160px
		a.Service-link.Si
			background Green_Aly
			padding 5px
		a.Service-link.No
			background Red_Mejor
			padding 5px	
/* #TOPVALLENATO */
	section#Avatar-CE.Avatar-CE
		background white	
		background-position left
		background-repeat no-repeat
		background-size cover
		height auto
		margin-top 47px
		overflow hidden 
		padding 0
		text-align: center			
		figure.Avatar-containerCE
			margin 0px
		// .Avatar-img
		// 	background rgba(255, 255, 255, 0.38)
		// 	border 5px solid black
		// 	border-radius 10%
