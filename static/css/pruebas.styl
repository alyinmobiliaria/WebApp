flexbox()
   display -webkit-box
   display -moz-box
   display -ms-flexbox
   display -webkit-flex
   display flex
order()
   -webkit-box-ordinal-group arguments
   -moz-box-ordinal-group arguments
   -ms-flex-order arguments
   -webkit-order arguments
   order arguments

footer
header
   background #b2b2b2
   overflow hidden
   padding 10px
   text-align center

footer
   line-height 25px

h1
   margin 0

header
   line-height 75px

#wrapper
   display -webkit-box
   display -moz-box
   display -ms-flexbox
   display -webkit-flex
   display flex
   flex-flow row

   aside
   section
      display -webkit-box
      display -moz-box
      display -ms-flexbox
      display -webkit-flex
      display flex
      flex-grow 1
      article
      div
         color #fdfdfd
         background #666666
         flex-grow 1
         margin 10px
         padding 5px
         h2
            margin 0
         p
            margin 5px 0

   aside
      flex-flow column
      background #cccccc
      width 25%

   section
      flex-flow row
      width 75%

@media screen and (max-width: 1000px) and (min-width: 640px)
   #wrapper
      section
         flex-flow column

@media screen and (max-width: 640px)
   #wrapper
      flex-flow column
      aside
         -webkit-box-ordinal-group 2
         -moz-box-ordinal-group 2
         -ms-flex-order 2
         -webkit-order 2
         order 2
         width auto
      section
         flex-flow column
         -webkit-box-ordinal-group 1
         -moz-box-ordinal-group 1
         -ms-flex-order 1
         -webkit-order 1
         order 1
         width auto



@media screen and (min-width:220px)
   html
      background black
      body
         background white
      nav.Canvas
         left 0
         // overflow hidden
         position fixed
         right 0 
         top 0
         z-index 6
      .Header
         background black
         color white
         height 30px
         line-height 30px
         padding 10px 0
         position relative 
         &-logoCon
            margin 0
            top 0
            width 200px
         &-logoTexto
            display block
            font-size 30px
            left 60px
            line-height 40px
            margin 0
            position absolute
            text-shadow 0 0 10px lightgray
            top 6px
         &-logo1
            display block
            left 5px
            margin 0
            position absolute
            top 2px
            z-index 10px
            
         &-button
            background black
            border none 
            color white
            display none
            font-size 30px
            font-weight normal
            padding 0
            position absolute
            right 5px
            top 10px
            &.is-active
               display block 
      .MainMenu
         display visible
         background black
         height auto
         left -200px
         list-style none
         margin 0
         padding 0
         position absolute
         position fixed
         top 50px
         transition .2s
         width 165px
         &.is-active
            left 0
         &-item
            -webkit-transition .2s
            height 50px
            line-height 50px
            text-align center
            text-shadow 0 0 10px Verde_D
            &:hover
               background lightgray 
         &-link
            color white
            display block
            font-size 14px
            text-decoration none
            text-transform uppercase
